package linkedList;

public class LinkedListInsertion {
	Node head;
	
	public void add(int d){
		Node new_node=new Node(d);
		if(head==null){
			head=new_node;
			return;
		}
		else{
			new_node.next=head;
			head=new_node;
		}
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
