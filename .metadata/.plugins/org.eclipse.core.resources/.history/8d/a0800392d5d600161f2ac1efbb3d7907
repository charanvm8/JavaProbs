package netLogic;

public class Next10 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//area of triangle
		
		double base=2;
		double height=6;
		
		System.out.println(0.5*base*height);
		
		
		// using if
		
		// prime numbers upto 100
		
		int x=11;
		int count=0;
		for(int i=1;i<=100;i++){
			
			if(x%i == 0){
				count++;
			}
			
		}
		if(count==2){
			System.out.println("prime number" +x);
		}
		
		// fibanocci seq
		
		int q=0;
		int z=1;
		
		int num=10;
		
		for(int i=1;i<=10;i++){
			q=q+z;
			z=q+z;
			System.out.print(q+" "+z+" ");
			
		}

	}

}
