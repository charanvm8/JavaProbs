package practice;

public class Practice1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a=0;
		int b=1;
		int sum=0;
		int total=0;
		
		for(int i=1;i<=4000000;i++){
			sum=a+b;
			
			if(sum%2==0)
				total+=sum;
			a=b;
			b=sum;
			
		}
		System.out.println(total);
	}

}
