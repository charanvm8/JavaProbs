package netLogic;

public class Next10 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//area of triangle
		
		double base=2;
		double height=6;
		
		System.out.println(0.5*base*height);
		
		
		// using if
		
		// prime numbers upto 100
		
		int x=11;
		
		for(int i=1;i<=100;i++){
			int count=0;
			if(x%i == 0){
				count++;
			}
			if(count==2){
				System.out.println("prime number");
			}
		}

	}

}
